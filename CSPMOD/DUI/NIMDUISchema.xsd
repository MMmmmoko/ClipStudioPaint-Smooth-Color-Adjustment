<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NIMDUISchema"
    targetNamespace="http://tempuri.org/NIMDUISchema.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/NIMDUISchema.xsd"
    xmlns:mstns="http://tempuri.org/NIMDUISchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
	
	<!--针对nimduilib(民间维护的基于网易的duilib的跨平台版本)的XSD文件-->
	<!--https://github.com/rhett-lee/nim_duilib/blob/main/docs/Control.md-->

	
	<!--************经常修改的部分，Global文件中的Color*************-->

		<xs:simpleType name="COLOR">
		<xs:union memberTypes="xs:string COLOR_NAME"/>
	</xs:simpleType>
	<xs:simpleType name="COLOR_NAME">
		<xs:restriction base="xs:string">
			<xs:enumeration value="white"/>
			<xs:enumeration value="black"/>
			<xs:enumeration value="darkcolor"/>
			<xs:enumeration value="lightcolor"/>
			<xs:enumeration value="gray"/>
			<xs:enumeration value="light_gray"/>
			<xs:enumeration value="dark_gray"/>
			<xs:enumeration value="green"/>
			<xs:enumeration value="light_green"/>
			<xs:enumeration value="blue"/>
			<xs:enumeration value="red"/>
		</xs:restriction>
	</xs:simpleType>

		<!--*************************-->
	<xs:simpleType name="INT_STRING">
		<xs:union memberTypes="xs:integer xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="RECT">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d+,\d+,\d+,\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SIZE">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d+,\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="INT_RECT">
		<xs:union memberTypes="xs:integer RECT"/>
	</xs:simpleType>
	<xs:simpleType name="STRING">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="INT">
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="BOOL">
		<xs:restriction base="xs:string">
			<xs:enumeration value="true"/>
			<xs:enumeration value="false"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DOUBLE">
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="DWORD">
		<xs:restriction base="xs:string">
			<xs:pattern value="0x[0-9A-Fa-f]{8}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="INT_WIDTH_HEIGHT">
		<xs:union memberTypes="xs:integer WIDTH_HEIGHT_STR PERCENTAGE"/>
	</xs:simpleType>
	<xs:simpleType name="WIDTH_HEIGHT_STR">
		<xs:restriction base="xs:string">
			<xs:enumeration value="stretch"/>
			<xs:enumeration value="auto"/>
		</xs:restriction>
	</xs:simpleType> 
	<xs:simpleType name="PERCENTAGE">
  <xs:restriction base="xs:string">
    <xs:pattern value="\d+%"/>
  </xs:restriction>
</xs:simpleType>
		
	<xs:simpleType name="HALIGN">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VALIGN">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="bottom"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CONTROL_STATE">
		<xs:restriction base="xs:string">
			<xs:enumeration value="normal"/>
			<xs:enumeration value="hot"/>
			<xs:enumeration value="pushed"/>
			<xs:enumeration value="disabled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CURSOR_TYPE">
		<xs:restriction base="xs:string">
			<xs:enumeration value="arrow"/>
			<xs:enumeration value="hand"/>
			<xs:enumeration value="wait"/>
			<xs:enumeration value="cross"/>
			<xs:enumeration value="ibeam"/>
			<xs:enumeration value="size_we"/>
			<xs:enumeration value="size_ns"/>
			<xs:enumeration value="size_nwse"/>
			<xs:enumeration value="size_nesw"/>
			<xs:enumeration value="size_all"/>
			<xs:enumeration value="no"/>
			<xs:enumeration value="progress"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TEXT_ALIGN">
		<xs:union memberTypes="xs:string TEXT_ALIGN_ENUM"/>
	</xs:simpleType>
	<xs:simpleType name="TEXT_ALIGN_ENUM">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="hcenter"/>
			<xs:enumeration value="vcenter"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="bottom"/>
			<xs:enumeration value="hcenter,vcenter"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RICH_EDIT_ALIGN">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="hcenter"/>
			<xs:enumeration value="top"/>
			<xs:enumeration value="vcenter"/>
			<xs:enumeration value="bottom"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="IMG_HINT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="xxx.png"/>
			<xs:enumeration value="file='aaa.jpg' dest='0,0,0,0' source='0,0,0,0' corner='0,0,0,0' fade='255' xtiled='false' ytiled='false'"/>
			<xs:enumeration value="file='aaa.jpg' dest='0,0,0,0' source='0,0,0,0' corner='0,0,0,0' fade='255' xtiled='false' ytiled='false' playcount='-1' antialiasing='false'"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IMG">
		<xs:union memberTypes="xs:string IMG_HINT"/>
	</xs:simpleType>
	
	<xs:simpleType name="COMBO_TYPE">
			<xs:restriction base="xs:string">
			<xs:enumeration value="drop_list"/>
			<xs:enumeration value="drop_down"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="SHADOW_HINT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="color='red' offset='0,0' blurradius='8' spreadradius='8'"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SHADOW">
		<xs:union memberTypes="xs:string SHADOW_HINT"/>
	</xs:simpleType>

	<xs:simpleType name="SHADOW_TYPE">
			<xs:restriction base="xs:string">
			<xs:enumeration value="default"/>
			<xs:enumeration value="big"/>
			<xs:enumeration value="big_round"/>
			<xs:enumeration value="small"/>
			<xs:enumeration value="small_round"/>
			<xs:enumeration value="menu"/>
			<xs:enumeration value="menu_round"/>
			<xs:enumeration value="none"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DATE_FORMAT">
			<xs:restriction base="xs:string">
			<xs:enumeration value="date_calendar"/>
			<xs:enumeration value="date_up_down"/>
			<xs:enumeration value="date_time_up_down"/>
			<xs:enumeration value="date_minute_up_down"/>
			<xs:enumeration value="time_up_down"/>
			<xs:enumeration value="minute_up_down"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="COLOR_DIRECTION">
			<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DASH_STYLE">
			<xs:restriction base="xs:string">
			<xs:enumeration value="solid"/>
			<xs:enumeration value="dash"/>
			<xs:enumeration value="dot"/>
			<xs:enumeration value="dash_dot"/>
			<xs:enumeration value="dash_dot_dot"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TRIM_POLICY">
			<xs:restriction base="xs:string">
			<xs:enumeration value="all"/>
			<xs:enumeration value="none"/>
			<xs:enumeration value="keep_one"/>
		</xs:restriction>
	</xs:simpleType>
				
	<xs:simpleType name="FONT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="system_12"/>
			<xs:enumeration value="system_14"/>
			<xs:enumeration value="system_16"/>
			<xs:enumeration value="system_18"/>
			<xs:enumeration value="system_20"/>
			<xs:enumeration value="system_22"/>
			<xs:enumeration value="system_bold_12"/>
			<xs:enumeration value="system_bold_14"/>
			<xs:enumeration value="system_bold_16"/>
			<xs:enumeration value="system_bold_18"/>
			<xs:enumeration value="system_bold_20"/>
			<xs:enumeration value="system_bold_22"/>
			<xs:enumeration value="arial_12"/>
			<xs:enumeration value="arial_14"/>
			<xs:enumeration value="arial_16"/>
			<xs:enumeration value="arial_18"/>
			<xs:enumeration value="arial_20"/>
			<xs:enumeration value="arial_22"/>
			<xs:enumeration value="arial_bold_12"/>
			<xs:enumeration value="arial_bold_14"/>
			<xs:enumeration value="arial_bold_16"/>
			<xs:enumeration value="arial_bold_18"/>
			<xs:enumeration value="arial_bold_20"/>
			<xs:enumeration value="arial_bold_22"/>
			<xs:enumeration value="system_underline_12"/>
			<xs:enumeration value="system_underline_14"/>
			<xs:enumeration value="system_underline_16"/>
			<xs:enumeration value="system_underline_18"/>
			<xs:enumeration value="system_underline_20"/>
			<xs:enumeration value="system_underline_22"/>
		</xs:restriction>
	</xs:simpleType>
	
	
